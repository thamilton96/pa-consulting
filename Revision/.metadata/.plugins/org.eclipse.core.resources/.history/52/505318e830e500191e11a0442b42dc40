package revision;

public class CodingBat {

	public static void main(String[] args) {

		CodingBat codingBat = new CodingBat();
		
		boolean outcome = codingBat.stringE("Hello");
		System.out.println(outcome);
	}
	
	public boolean stringE(String word) {
		boolean result = true;
		
		
		if(word.contains("e") || word.contains("ee") || word.contains("eee")) {
			result = true;
		}
		else {
			result = false;
		}
		
		return result;
	}

}
