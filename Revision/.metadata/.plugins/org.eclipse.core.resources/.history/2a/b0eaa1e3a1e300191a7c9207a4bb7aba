package revision;

public class FizzAray2 {

	public static void main(String[] args) {
		public String[] fizzArray2(int n) {
			  String[] result = new String[n];
			  
			  for(int index = 0; index < n; index++) {
			    
			    result[index] = String.valueOf(index);
			  }
			  
			  return result;
			}

	}

}
