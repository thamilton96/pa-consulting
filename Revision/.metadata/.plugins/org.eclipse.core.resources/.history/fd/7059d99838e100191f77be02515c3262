package revision;


public class Nth {
	
	String s;
	int n;


	public static void main(String[] args) {
		
		Nth nth = new Nth(); 
		
		String outcome = nth.everyNth("Miracle", 2); //gives "Mrce"
		System.out.println(outcome);
		
		String outcome1 = nth.everyNth("abcdefg", 2); //gives "aceg"
		System.out.println(outcome);
		
		String outcome2 = nth.everyNth("abcdefg", 3); //gives "adg"
		System.out.println(outcome);	
		
		if(outcome = "Mrce" && outcome1 == "aceg" && outcome2 == "adg") {
			System.out.println("It worked");
			
		}

	}

	public String everyNth(String word, int index) {
		
	
		for(int index1 =0; index1 < word.size() ; index1++) {
			System.out.println(word);
		}
		}

		//return result.charAt(3);
		
	}

	

